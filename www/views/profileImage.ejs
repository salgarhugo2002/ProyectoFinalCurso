<%- include("./templates/cabecera") %>
</head>

<body>
    <div class="container p-4">
        <div class="row">
            <div class="col-md-4 offset-md-4">
                <div class="card">
                    <img src="<%= image.path %>" class="card-img-top" alt="">
                    <div class="cars-body">
                        <h5 class="card-title">
                            <%= image.title %>
                        </h5>
                        <p class="card-text">
                            <%= image.description %>
                        </p>
                        <p class="card-text">
                            <%= format(image.created_at) %>
                        </p>
                        <a href="/image/<%= image._id %>/delete" class="btn btn-danger">Delete Image</a>
                    </div>
                </div>
            </div>
           


        </div>
    </div>
</body>

</html>